            <Color x:Key="TextFillColorPrimary">#1A1A1A</Color>
            <Color x:Key="TextFillColorSecondary">#5C5C5C</Color>
            <Color x:Key="TextFillColorTertiary">#8A8A8A</Color>
            <Color x:Key="TextFillColorDisabled">#C4C4C4</Color>
            
            <Color x:Key="ControlStrokeColorDefault">#E5E5E5</Color>
            <Color x:Key="ControlStrokeColorSecondary">#EBEBEB</Color>
            
            <Color x:Key="SystemSuccessColor">#0F7B0F</Color>
            <Color x:Key="SystemWarningColor">#9D5D00</Color>
            <Color x:Key="SystemErrorColor">#C42B1C</Color>
            <Color x:Key="SystemCautionColor">#424242</Color>
            
