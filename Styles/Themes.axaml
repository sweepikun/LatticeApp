<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <Color x:Key="SystemAccentColor">#0078D4</Color>
            <Color x:Key="SystemAccentColorLight">#60CDFF</Color>
            <Color x:Key="SystemAccentColorDark">#005A9E</Color>
            
            <Color x:Key="LayerFillColorDefault">#F3F3F3</Color>
            <Color x:Key="CardBackground">#FFFFFF</Color>
            <Color x:Key="CardBackgroundHover">#F5F5F5</Color>
