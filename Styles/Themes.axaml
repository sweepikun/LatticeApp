            <SolidColorBrush x:Key="StatusErrorBrush" Color="{StaticResource SystemErrorColor}"/>
            <SolidColorBrush x:Key="StatusStoppedBrush" Color="{StaticResource SystemCautionColor}"/>
            
            <SolidColorBrush x:Key="NavigationViewItemBackground" Color="Transparent"/>
            <SolidColorBrush x:Key="NavigationViewItemBackgroundHover" Color="#2A2A2A"/>
            <SolidColorBrush x:Key="NavigationViewItemBackgroundSelected" Color="#2D2D2D"/>
            <SolidColorBrush x:Key="NavigationViewItemBorderSelected" Color="{StaticResource SystemAccentColor}"/>
            
            <SolidColorBrush x:Key="WindowCloseButtonHover" Color="#C42B1C"/>
            <SolidColorBrush x:Key="WindowTitleBarButtonHover" Color="#2A2A2A"/>
